{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Us≈Çugi",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 70,
      "minLength": 3
    },
    "description": {
      "type": "text",
      "required": true,
      "maxLength": 1000,
      "minLength": 3
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "services"
    },
    "application": {
      "displayName": "application",
      "type": "component",
      "repeatable": true,
      "component": "page.application"
    },
    "oldPrice": {
      "type": "decimal"
    }
  }
}
